<mvc:View xmlns:tnt="sap.tnt"
xmlns:form="sap.ui.layout.form"



 xmlns:l="sap.ui.layout"
 controllerName="nauticalfe.controller.createVoyageTypeMast"
    xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" displayBlock="true">
	
  <Page id="_IDGenPage1" title="Editable Table">
    <content>
      <Table id="myTable" mode="None" items="{dkdsk}">
        <columns>
          <Column id="_IDGenColumn1">
            <Text id="_IDGenText1" text="Voyage Code" />
          </Column>
          <Column id="_IDGenColumn2">
            <Text id="_IDGenText2" text="Description" />
          </Column>
          <Column id="_IDGenColumn3">
            <Text id="_IDGenText3" text="Actions" />
          </Column>
        </columns>
        <items>
          <ColumnListItem id="_IDGenColumnListItem1">
            <cells>
              <Input id="_IDGenInput1" value="{VOYCD}" editable="true" />
              <Input id="_IDGenInput2" value="{VOYDES}" editable="true" />
              <Button id="_IDGenButton1" icon="sap-icon://delete" press="onDeleteRow" />
            </cells>
          </ColumnListItem>
        </items>
      </Table>
      <Button id="_IDGenButton2" text="Add Row" press="onAddRow" />
    </content>
  </Page>

<!--scrollbar added-->
<ScrollContainer id="_IDGenScrollContainer1"
		height="100%"
		width="100%"
		vertical="true"
		focusable="true">
  <!--this is first page-->
        
   	</ScrollContainer>   
</mvc:View>