<mvc:View xmlns:tnt="sap.tnt"
xmlns:form="sap.ui.layout.form"



 xmlns:l="sap.ui.layout"
 controllerName="nauticalfe.controller.createVoyageTypeMast"
    xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" displayBlock="true">
	
  <Page id="_IDGenPage1" >
    <content>
    <ObjectHeader id="_IDGenObjectHeader1" title="Create Voyage Type" icon="sap-icon://document"></ObjectHeader>
    <Panel id="_IDGenPanel1" >
    <Bar id="_IDGenBar1" >
      <contentLeft>
      
      <Button id="_IDGenButton3" text="Create new" press="onCreate"></Button>
      <Button id="_IDGenButton4" text="Delete" press="onDeleteVoyageType"></Button>
      
      <Button id="_IDGenButton6" text="update" press="onUpdate"></Button>

      </contentLeft>
    </Bar>
      <Table id="createTypeTable" mode="MultiSelect" items="{/VOYTYP}"  updateFinished="onTableUpdateFinished" selectionChange="selectedItems" >

        <columns>
          <Column id="_IDGenColumn1" >
            <Text id="_IDGenText1" text="Voyage Code"  />
          </Column>
          <Column id="_IDGenColumn2">
            <Text id="_IDGenText2" text="Description" />
          </Column>
         
        </columns>
        <items>
          <ColumnListItem id="_IDGenColumnListItem1">
            <cells>
              <Input id="_IDGenInput1" value="{VOYCD}" editable="false"  />
              <Input id="_IDGenInput2" value="{VOYDES}" editable="false" />
              <Button id="_IDGenButton1" icon="sap-icon://delete" press="onDeleteRow" />
            </cells>
          </ColumnListItem>
         
        </items>
      </Table>
      <Button id="_IDGenButton2" text="Add Row" press="onAddRow" />
    </Panel>
    </content>
  </Page>

<!--scrollbar added-->
<ScrollContainer id="_IDGenScrollContainer1"
		height="100%"
		width="100%"
		vertical="true"
		focusable="true">
  <!--this is first page-->
        
   	</ScrollContainer>   
</mvc:View>